    "memory": {
      "clearBetweenVideos": true,
      "preloadNext": false,
      "maxCacheSize": "100MB"
    },
    "video": {
      "preload": "auto",
      "playsInline": true,
      "muted": true,
      "controls": false
    }
  }
}