{
  "systemName": "Android TV CODECRAFT",
  "transitionTime": 5,
  "defaultVolume": 30,
  "tvOptimizations": {
    "enabled": true,
    "autoplay": {
      "strategy": "aggressive",
      "retryAttempts": 3,
      "retryDelay": 2000
